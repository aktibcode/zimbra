	<queryString language="SQL">
		<![CDATA[SELECT distinct *
FROM heritier.note, heritier.validationtotal,
	 heritier.eleve, 
	heritier.cours
	INNER JOIN heritier.type_matiere ON 
	 cours.id_ty = heritier.type_matiere.id_ty 
WHERE 
	 
	 note.`Semestre` = $P{pSem} 
	 AND note.classe = $P{pClasse} 
	 AND note.anne_scolaire = $P{pAnSco} 
	AND cours.cr_titre = note.matiere 
	and validationtotal.Num =  note.id_elv
	and validationtotal.`Semestre` = $P{pSem} 
	and validationtotal.`Classe` = $P{pClasse} 
	and validationtotal.`Annee` =  $P{pAnSco} 
having note.id_elv = $P{pID} and validationtotal.Num =  $P{pID} and  heritier.eleve.elv_id = $P{pID} ]]>
	</queryString>
