			<![CDATA[select distinct moyeleve.Num, nomEtprenom,
 moyeleve.Classe, moyeleve.MOY as M1, moyeleve1.MOY as M2, moyeleve2.MOY as M3, anne_scolaire , elv_id 

from moyeleve, moyeleve1, moyeleve2, eleve 

where moyeleve.Classe = '4EB' and moyeleve.Num = moyeleve1.Num and moyeleve1.Num = moyeleve2.Num and moyeleve2.Num = elv_id and moyeleve.Num = 124 and anne_scolaire = ' 2018-2019']]>
