	<queryString language="SQL">
		<![CDATA[select distinct  note_id, note3.anne_scolaire, note3.Semestre, note3.classe, nom_et_prenom, note3.matiere, note3.coef, note_Examen, moy_cls, moy_Gen, cr_id, cr_titre, cr_coef, titre,

type_matiere.id_ty, cours.cr_titre, note3.id_elv, note3.coefClasse, note1, note2, DS_1, DS_2  , cours.id_ty, eleve.elv_id,
rang.MOY as MY , rang.rang1 as rg , rang.Num as nu, stockage.M1, stockage.M2, stockage.M3, stockage.Num, validationtotal.Num

from validationtotal, note3,  eleve, cours ,   type_matiere ,rang , stockage
  
  where 
 cours.cr_titre =note3.matiere
 and cours.id_ty = type_matiere.id_ty

  and validationtotal.Num = note3.id_elv 
 AND note3.`Semestre` = $P{pSem} 
AND note3.classe =   $P{pClasse}   
AND note3.anne_scolaire = $P{pAnSco} 
 AND validationtotal.`Semestre` = $P{pSem} 
 and  stockage.Num =eleve.elv_id
 
 and rang.Num = validationtotal.Num

having      cours.cr_titre =note3.matiere  AND   eleve.elv_id =  validationtotal.Num and  stockage.Num =eleve.elv_id]]>
