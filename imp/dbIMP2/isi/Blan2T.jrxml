	<queryString language="SQL">
		<![CDATA[select distinct  * , rang.MOY as MY , rang.rang1 as rg , rang.Num as nu 

from validationtotal, note2,  eleve, cours ,   type_matiere ,rang 
  
  where 
 cours.cr_titre =note2.matiere
 and cours.id_ty = type_matiere.id_ty

  and validationtotal.Num = note2.id_elv 
 AND note2.`Semestre` = $P{pSem} 
AND note2.classe =   $P{pClasse}   
AND note2.anne_scolaire = $P{pAnSco} 
 AND validationtotal.`Semestre` = $P{pSem} 
 AND   eleve.ID_ETUDIANT =  validationtotal.Num
 and rang.Num = validationtotal.Num
having     cours.id_ty = type_matiere.id_ty AND   eleve.ID_ETUDIANT=  validationtotal.Num]]>
