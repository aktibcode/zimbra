	<queryString language="SQL">
		<![CDATA[select distinct validationtotal.Num, note_id, note3.anne_scolaire, note3.classe, nom_et_prenom, `matiere`,coef,`note1`,`note2`,`DS_1`,`DS_2`,`note_Examen`, moy_cls, moy_Gen, id_elv, cr_id, cr_titre, cr_coef, titre,
note3.coefClasse, type_matiere.id_ty, stockage1.rang2 as r2, stockage1.rang1 as r1, stockage1.rang3 as r3,
nom_et_prenom , type_matiere.titre,
cours.cr_titre, note3.id_elv  , stockage1.M1, 
stockage1.M2 , stockage1.M3, stockage1.Num as nu , note3.Semestre, cours.id_ty,eleve.elv_id, validationtotal.`Semestre`,stockage1.anne_scolaire
from validationtotal, note3, stockage1,  eleve, cours ,   type_matiere
  
  where 
 cours.cr_titre =note3.matiere
 and cours.id_ty = type_matiere.id_ty

  and validationtotal.Num = note3.id_elv 
 AND note3.`Semestre` = $P{pSem} 
AND note3.classe =   $P{pClasse}   
AND note3.anne_scolaire = $P{pAnSco}  and stockage1.anne_scolaire = $P{pAnSco} 
 AND validationtotal.`Semestre` = $P{pSem} 
 AND   eleve.elv_id =  validationtotal.Num
 and stockage1.Num = validationtotal.Num
having     cours.id_ty = type_matiere.id_ty AND   eleve.elv_id =  validationtotal.Num]]>
