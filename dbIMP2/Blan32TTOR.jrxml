	<queryString language="SQL">
		<![CDATA[SELECT distinct 
note_id, note3.anne_scolaire, note3.classe, nom_et_prenom, `matiere`,coef,`note1`,`note2`,`DS_1`,`DS_2`,`note_Examen`, moy_cls, moy_Gen, id_elv, cr_id, cr_titre, cr_coef, titre,
note3.coefClasse, type_matiere.id_ty, stockage1.rang2 as rg2, stockage1.rang1 as rg1, stockage1.rang3 as rg3,
nom_et_prenom , type_matiere.titre,
cours.cr_titre, note3.id_elv  , stockage1.M1, 
stockage1.M2 , stockage1.M3, stockage1.Num as nu , note3.Semestre

FROM `note3`, cours, type_matiere , validationtotal, stockage1


WHERE note3.id_elv = $P{pID}  and type_matiere.id_ty = cours.id_ty 
and note3.Semestre =  $P{pSem}  

and  validationtotal.Num =$P{pID}
and  stockage1.Num = $P{pID} 


having  note3.matiere = cours.cr_titre]]>
